{
  "name": "cash-app-webasyst",
  "private": true,
  "scripts": {
    "lint": "vue-cli-service lint",
    "build-desktop": "vue-cli-service build src/desktop.js --dest dist/desktop --mode desktop.production && npm run copycss && npm run copyjs && npm run build-widgets",
    "build-mobile": "vue-cli-service build src/mobile.js --dest dist/mobile --mode mobile.production",
    "build-widgets": "vue-cli-service build --target lib --formats umd-min --no-clean --dest ../widgets/balanceflow/js --name chartsD3Lib src/utils/currencyChartD3.js",
    "build-storybook": "build-storybook",
    "copycss": "copyfiles dist/desktop/css/app.css ../css --flat",
    "copyjs": "copyfiles dist/desktop/js/app.js ../js --flat",
    "serve-desktop": "vue-cli-service serve src/desktop.js --mode desktop.development",
    "serve-mobile": "vue-cli-service serve src/mobile.js --mode mobile.development",
    "storybook": "start-storybook -p 6006"
  },
  "dependencies": {
    "@amcharts/amcharts4": "^4.10.22",
    "@jaames/iro": "^5.5.2",
    "@linusborg/vue-simple-portal": "^0.1.5",
    "@uppy/core": "^1.20.1",
    "@uppy/drag-drop": "^1.4.31",
    "@uppy/xhr-upload": "^1.7.5",
    "axios": "^0.20.0",
    "core-js": "^3.19.1",
    "d3": "^6.7.0",
    "flatpickr": "^4.6.9",
    "microtip": "^0.2.2",
    "moment": "^2.29.1",
    "numeral": "^2.0.6",
    "vue": "^2.6.14",
    "vue-click-outside": "^1.1.0",
    "vue-i18n": "^8.26.7",
    "vue-imask": "^6.2.2",
    "vue-meta": "^2.4.0",
    "vue-router": "^3.5.3",
    "vuedraggable": "^2.24.3",
    "vuelidate": "^0.7.6",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/addon-viewport": "^6.3.12",
    "@storybook/vue": "^6.3.12",
    "@vue/cli-plugin-babel": "~4.5.15",
    "@vue/cli-plugin-eslint": "~4.5.15",
    "@vue/cli-plugin-router": "~4.5.15",
    "@vue/cli-plugin-vuex": "~4.5.15",
    "@vue/cli-service": "~4.5.15",
    "@vue/composition-api": "^1.3.3",
    "@vue/eslint-config-standard": "^5.1.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.3",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^4.1.0",
    "eslint-plugin-vue": "^6.2.2",
    "node-fetch": "^2.6.6",
    "sass": "^1.43.4",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
